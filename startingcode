//code for workcube 

#include <PRIZM.h>   //link up the library
PRIZM prizm;        //declare object (refence to prism)

void setup(){
  przim.PrizmBegin();
  prizm.setMotorPower(1,100);   //100% speed
  delay(2000);
  prizm.setMotorPower(1,125); //125 means brake
  delay(2000);

}
void loop (){
 
}
